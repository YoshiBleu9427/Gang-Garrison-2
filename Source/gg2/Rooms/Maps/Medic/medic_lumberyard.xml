  <creationCode>basicRoomSetup();
instance_create(0,0,LumberyardO);
with(NPC01) {
    createAllyHeavy();
    npcSetTask(NPC_TASK_PET, global.myself.object);
    briefing = "You! Yes, you!";
    briefing2 = "Good day! You are healer!#You are credit to team!";
    briefing3 = "Healer! Entire team must be alive!#If we dead, you are bad healer!";
    deathTxt = "Aaaahahaaaa!#Not good!";
    
    addDialogForce(briefing, face, subFace);
    addDialogForce(briefing2, face, subFace);
    addDialogForce(briefing3, face, subFace);
    
    object_event_add(object_index, ev_other, NPC_EVENT_DEATH, '
        addPriorDialog(deathTxt, face, subFace);
        gameOver("GAME OVER#Your ally died.#Do not let your team die!");
    ');
}
with(NPC02) {
    createAllySoldier();
    npcSetTask(NPC_TASK_PET, global.myself.object);
    briefing = "We need to stay together,#healer!";
    briefing2 =  "We'll charge as soon as#we see the enemy!"
    deathTxt = "Must... resist...#Must not... arrrrrr";
    
    addDialogForce(briefing, face, subFace);
    addDialogForce(briefing2, face, subFace);
    
    object_event_add(object_index, ev_other, NPC_EVENT_DEATH, '
        addPriorDialog(deathTxt, face, subFace);
        gameOver("GAME OVER#Your ally died.#Do not let your team die!");
    ');
}
with(NPC03) {
    createEnemyScout();
    npcSetTask(NPC_TASK_STAY, noone);
    deathTxt = "I apologize for everything!#I apologize for everything#I've ever done!";
    seeTxt = "Yo, they're comin'!";
    
    object_event_add(object_index, ev_other, NPC_EVENT_DEATH, '
        addPriorDialog(deathTxt, face, subFace);
    ');
    
    object_event_add(object_index, ev_other, NPC_EVENT_PLAYER_SEEN, '
        addDialog(seeTxt, face, subFace);
        with(EventManager) event_user(15); 
    ');
}
with(NPC04) {
    createEnemyPyro();
    npcSetTask(NPC_TASK_STAY, noone);
    deathTxt = "MMMPFH";
    seeTxt = "Mmmph!";
    
    object_event_add(object_index, ev_other, NPC_EVENT_DEATH, '
        addPriorDialog(deathTxt, face, subFace);
    ');
    
    object_event_add(object_index, ev_other, NPC_EVENT_PLAYER_SEEN, '
        addDialog(seeTxt, face, subFace);
        with(EventManager) event_user(15); 
    ');
}
with(NPC05) {
    createEnemySniper();
    npcSetTask(NPC_TASK_STAY, noone);
    deathTxt = "This is getting#embarr-ARRRR";
    seeTxt = "Let's do this no-scope style!";
    
    object_event_add(object_index, ev_other, NPC_EVENT_DEATH, '
        addPriorDialog(deathTxt, face, subFace);
    ');
    
    object_event_add(object_index, ev_other, NPC_EVENT_PLAYER_SEEN, '
        addDialog(seeTxt, face, subFace);
        with(EventManager) event_user(15); 
    ');
}
with(NPC06) {
    createEnemySoldier();
    npcSetTask(NPC_TASK_STAY, noone);
    
    object_event_add(object_index, ev_other, NPC_EVENT_PLAYER_SEEN, '
        with(EventManager) event_user(15); 
    ');
}

with(EventManager) {
    ev15Triggered = false;
    object_event_add(object_index, ev_other, PLAYER_EVENT_DEATH, '
        with(NPC01) {
            addPriorDialog("Healer!#NOOOOOO!", face, subFace);
        }
        gameOver();
    ');
    object_event_add(object_index, ev_other, ev_user15, '
        if(not ev15Triggered) {
            ev15Triggered = true;
            with(NPC) {
                if(team != global.myself.team) {
                    npcSetTask(NPC_TASK_CHASE, noone);
                }
            }
            toggleActionMusic(true);
        }
    ');
}
</creationCode>
