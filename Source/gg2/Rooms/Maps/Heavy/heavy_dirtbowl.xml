  <creationCode>basicRoomSetup();
instance_create(0,0,heavy_dirtbowlO);
global.respawntime = 30 * 4; // 5 sec respawn
global.setupTimer = 1; // no setup time
ControlPoint1.team = TEAM_RED;
ControlPoint2.locked = false;
ControlPointHUD.timer = 93 * 30; // defend the point for 93 seconds!
toggleActionMusic(true);
setPlayerTeam(TEAM_BLUE);
with(NPC01) {
    createAllySoldier();
    object.hp = 105;
    npcSetTask(NPC_TASK_CHASE);
    addDialog("Come here, sissy!", face, subFace);
    addDialogForce("You! Newcomer!#We need to defend the point!", face, subFace);
}
with(NPC02) {
    createAllyMedic();
    object.hp = 65;
    npcSetTask(NPC_TASK_PET);
}
with(NPC03) {
    createEnemyScout();
    object.hp = 35;
    npcSetTask(NPC_TASK_GOAL_OFFENSIVE);
    maxModifier = 30;
}
with(NPC04) {
    createEnemyHeavy();
    object.hp = 95;
    npcSetTask(NPC_TASK_CHASE);
    maxModifier = 20;
}
with(NPC05) {
    createEnemyScout();
    object.hp = 12;
    name = "Jello scoot";
    npcSetTask(NPC_TASK_GOAL_OFFENSIVE);
    maxModifier = 60;
}
with(NPC06) {
    createEnemyPyro();
    npcSetTask(NPC_TASK_GOAL_OFFENSIVE);
    maxModifier = 20;
}
